#!/usr/bin/env bash
source $(cli loader)

help() {
    cat << EOF
Command
    integrate area .group

Summary
    Commands for finding areas of shapes.

Global Arguments
    --help -h        [Flag] : Show this message and exit.
    --self-test      [Flag] : Runs a self test.
EOF
}

export() {
    cli::export::import integrate .group

    math::square() {
        math::multiply $1 $1
    }

    cli::export math
}

self_test() {
    source /dev/stdin < <(cli::import cli_assert)
}

cli::load "$@"