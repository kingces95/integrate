#!/usr/bin/env bash
source /dev/stdin < <(cli loader)
source /dev/stdin < <(cli::import integrate .group)

help() {
    cat << EOF
Command
    integrate area .group

Summary
    Commands for finding areas of shapes.

Global Arguments
    --help -h        [Flag] : Show this message and exit.
    --self-test      [Flag] : Runs a self test.
EOF
}

main() {
    cli::export_import integrate .group
    cli::export integrate area
}

integrate::area::square() {
    integrate::multiply $1 $1
}

self_test() {
    source /dev/stdin < <(cli_assert)
}

cli::load "$@"