#!/usr/bin/env bash
source /dev/stdin < <(cli loader --library)

help() {
    cat << EOF
Command
    integrate .dir

Summary
    Command line tool for integrating shapes and volumes.

Global Arguments
    --help -h        [Flag] : Show this message and exit.
    --self-test      [Flag] : Runs a self test.
EOF
}

INTEGRATE_PI='3.14159'

integrate::multiply() {
    IFS='*' expr=$*; echo "$expr" | bc -l
}

self_test() {
    source /dev/stdin < <(cli_assert)
}

main() {
    cli::export integrate
}

cli::load "$@"