#!/usr/bin/env bash
source /dev/stdin < <(cli loader)
source /dev/stdin < <(cli initialize)
source /dev/stdin < <(cli export)

help() {
    cat << EOF
Command
    integrate .dir

Summary
    Command line tool for integrating shapes and volumes.

Global Arguments
    --help -h        [Flag] : Show this message and exit.
    --self-test      [Flag] : Runs a self test.
EOF
}

main() {
    cli::export::import cli_math
}

self_test() {
    source /dev/stdin < <(cli::import cli_assert)
}

cli::load "$@"