#!/usr/bin/env bash
source /dev/stdin < <(cli loader --library)
source /dev/stdin < <(cli::import integrate .group)

help() {
    cat << EOF
Command
    integrate volume .group

Summary
    Commands for finding areas of shapes.

Global Arguments
    --help -h        [Flag] : Show this message and exit.
    --self-test      [Flag] : Runs a self test.
EOF
}

integrate::volume::cube() {
    integrate::multiply $1 $1 $1
}

self_test() {
    source /dev/stdin < <(cli_assert)
}

main() {
    cli::export_import integrate .group
    cli::export integrate volume
}

cli::load "$@"